{
  "name": "eurosport-react-test",
  "version": "0.1.0",
  "description": "React project for eurosport",
  "main": "src/index.js",
  "scripts": {
    "start": "npm-run-all --parallel dev server",
    "server": "json-server -p 9000 db.json",
    "dev": "NODE_ENV=development webpack-dev-server --open --hot",
    "build": "NODE_ENV=production webpack",
    "test": "jest",
    "format": "pretty-quick",
    "lint": "eslint ./",
    "lint:js": "npm run eslint-check && npm run format && eslint --config ./.eslintrc ./src/**/*.{js,jsx}",
    "lint:css": "npm run stylelint-check && npm run format:css && node_modules/.bin/stylelint --config ./.stylelintrc ./src/**/*.css",
    "eslint-check": "eslint --config ./.eslintrc --print-config . | eslint-config-prettier-check",
    "stylelint-check": "stylelint-config-prettier-check",
    "format:js": "prettier-eslint --write './src/**/*.{js,jsx}'",
    "format:css": "prettier-stylelint --write './src/**/*.css'"
  },
  "author": "Stephane Cottereau",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "autoprefixer": "^9.3.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "css-prefers-color-scheme": "^3.1.1",
    "cssnano": "^4.1.7",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^6.7.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.16.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "npm-run-all": "^4.1.5",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-prettify": "^0.3.4",
    "prettier": "^1.19.1",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "prettier-stylelint": "^0.4.2",
    "pretty-quick": "^2.0.1",
    "react-axe": "^3.3.0",
    "style-loader": "^1.0.0",
    "stylelint": "^12.0.0",
    "stylelint-a11y": "^1.1.9",
    "stylelint-checkstyle-formatter": "^0.1.2",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-prettier": "^1.0.5",
    "stylelint-webpack-plugin": "^0.10.5",
    "sugarss": "^2.0.0",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.7.0",
    "@hot-loader/react-dom": "^16.11.0",
    "core-js": "3",
    "json-server": "^0.15.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hot-loader": "^4.12.18",
    "react-props": "0.0.3",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  }
}
